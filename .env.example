# Flask Environment
FLASK_ENV=development

# API Configuration
API_HOST=0.0.0.0
API_PORT=5000

# CORS Configuration (comma-separated)
CORS_ORIGINS=*

# Logging
LOG_LEVEL=INFO

# Algorithm Defaults
DEFAULT_PATHFINDING_ALGORITHM=dijkstra
DEFAULT_DECISION_STRATEGY=meu
DEFAULT_LEARNING_STYLE=balanced

# Rate Limiting
RATE_LIMIT_ENABLED=true
RATE_LIMIT_DEFAULT=100 per hour
RATE_LIMIT_STORAGE_URL=memory://